(default "root")
(devolution "root"    :type 'virtual :deps '("lvl1:1" "lvl1:2"))
(devolution "lvl1:1"  :type 'virtual :deps '("lvl2:1"))
(devolution "lvl1:2"  :type 'virtual :deps '("lvl2:1" "lvl4:1"))
(devolution "lvl2:1"  :type 'virtual :deps '("lvl3:1" "lvl3:2"))
(devolution "lvl3:1"  :type 'virtual)
(devolution "lvl3:2"  :type 'virtual)
(devolution "lvl4:1"  :type 'virtual)
